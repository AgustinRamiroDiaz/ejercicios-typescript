{
  "name": "ejercicios-typescript",
  "version": "1.0.0",
  "description": "Ejercicios de TypeScript para aprender programación con temática de videojuegos",
  "type": "module",
  "scripts": {
    "test": "tsc && node --test './**/*.test.js'",
    "test:if": "tsc && node --test './01-if/*.test.js'",
    "test:while": "tsc && node --test './02-while/*.test.js'",
    "test:arrays": "tsc && node --test './03-arrays/*.test.js'",
    "test:for": "tsc && node --test './04-for/*.test.js'",
    "test:diccionarios": "tsc && node --test './05-diccionarios/*.test.js'",
    "test:objetos": "tsc && node --test './06-objetos/*.test.js'",
    "test:clases": "tsc && node --test './07-clases/*.test.js'",
    "test:funciones": "tsc && node --test './08-funciones/*.test.js'",
    "test:errores": "tsc && node --test './09-errores/*.test.js'",
    "test:recursion": "tsc && node --test './10-recursion/*.test.js'",
    "test:strategy": "tsc && node --test './11-strategy/*.test.js'",
    "test:tipos-avanzados": "tsc && node --test './12-tipos-avanzados/*.test.js'",
    "test:grafos": "tsc && node --test './13-grafos/*.test.js'"
  },
  "keywords": [
    "typescript",
    "ejercicios",
    "aprendizaje",
    "programacion"
  ],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/node": "^20.0.0",
    "ts-node": "^10.9.2",
    "typescript": "^5.0.0"
  }
}

